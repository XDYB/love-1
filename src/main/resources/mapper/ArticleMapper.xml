<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zcf.words.mapper.ArticleMapper">


    <select id="getArticel" resultType="com.zcf.words.entity.Article" >
        SELECT
            a.article_id,
            a.article_pic,
            a.article_title,
            a.create_time
        FROM
            article a
    </select>


</mapper>